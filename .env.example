# GitHub Configuration
GITHUB_TOKEN=ghp_your_github_personal_access_token
GITHUB_OWNER=Gzeu
GITHUB_REPO=autodevops-ai-orchestrator
GITHUB_WEBHOOK_SECRET=your_webhook_secret

# AI Configuration - Task Master
TASK_MASTER_API_KEY=your_task_master_api_key
TASK_MASTER_ENDPOINT=https://api.taskmaster.ai/v1
AI_MODEL=gpt-4
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2000

# FastMCP Configuration
MCP_HOST=localhost
MCP_PORT=3000
MCP_PROTOCOL=ws
MCP_AUTH_TOKEN=your_mcp_auth_token
MCP_TIMEOUT=30000

# Playwright Configuration
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_BROWSER=chromium
TEST_TIMEOUT=30000
TEST_RETRY_COUNT=2
TEST_PARALLEL_WORKERS=2

# Phoenix Monitoring Configuration
PHOENIX_ENDPOINT=https://your-phoenix-instance.com
PHOENIX_API_KEY=your_phoenix_api_key
MONITORING_WEBHOOK=https://your-webhook-url.com/webhook
METRICS_INTERVAL=30000
ALERT_THRESHOLD_ERROR_RATE=0.05

# Server Configuration
PORT=3001
HOST=localhost
NODE_ENV=development
LOG_LEVEL=info

# Database Configuration (Optional)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=autodevops_ai
DB_USER=postgres
DB_PASSWORD=your_db_password
DB_SSL=false

# Security
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key
API_RATE_LIMIT=100

# Docker Configuration
DOCKER_REGISTRY=docker.io
DOCKER_USERNAME=your_docker_username
DOCKER_PASSWORD=your_docker_password

# Notification Configuration
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url
EMAIL_SERVICE=gmail
EMAIL_USER=your-email@gmail.com
EMAIL_PASSWORD=your-app-password

# Performance Monitoring
PERF_MONITORING_ENABLED=true
MEMORY_THRESHOLD_MB=512
CPU_THRESHOLD_PERCENT=80

# Feature Flags
ENABLE_AI_CODE_GENERATION=true
ENABLE_AUTO_TESTING=true
ENABLE_AUTO_DEPLOYMENT=false
ENABLE_SECURITY_SCANNING=true
ENABLE_PERFORMANCE_MONITORING=true